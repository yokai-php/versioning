<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <defaults public="false"/>

        <service id="yokai.versioning.event_listener.init_context_listener"
                 class="Yokai\Versioning\Bridge\Symfony\EventDispatcher\EventListener\InitContextListener">
            <argument type="service" id="yokai.versioning.context"/>
            <argument type="service" id="security.token_storage" on-invalid="null"/>
        </service>
    </services>
</container>
